{
  "extends": ["prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "max-len": ["error", 120] // lines can be 120 characters long before they should break
  },
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "env": {
    "es6": true,
    "browser": true
  }
}